					Players.PlayerRemoving:Connect(function(player)
						if (CHMSenabled or COREGUI:FindFirstChild(player.Name .. "_LC")) then
							for i, v in pairs(COREGUI:GetChildren()) do
								if ((v.Name == (player.Name .. "_ESP")) or (v.Name == (player.Name .. "_LC")) or (v.Name == (player.Name .. "_CHMS"))) then
									v:Destroy();
								end
							end
						end
					end);
					RunService.Heartbeat:Connect(function(dt)
						if (following and followTarget) then
							rootPart.CFrame = rootPart.CFrame:Lerp(CFrame.new(followTarget.Position) * CFrame.new(0, 0, 3), 0.1);
						end
						if spinning then
							local FlatIdent_64F8C = 0;
							local spinAmount;
							while true do
								if (0 == FlatIdent_64F8C) then
									spinAmount = spinSpeed * 100 * dt;
									rootPart.CFrame = rootPart.CFrame * CFrame.Angles(0, math.rad(spinAmount), 0);
									break;
								end
							end
						end
						if (orbiting and orbitTarget) then
							local FlatIdent_1C000 = 0;
							local FlatIdent_2E48C;
							local offset;
							local newPosition;
							while true do
								if (FlatIdent_1C000 == 0) then
									local FlatIdent_6CDFC = 0;
									local FlatIdent_1414D;
									while true do
										if (FlatIdent_6CDFC == 0) then
											FlatIdent_1414D = 0;
											while true do
												if (FlatIdent_1414D == 0) then
													FlatIdent_2E48C = 0;
													offset = nil;
													FlatIdent_1414D = 1;
												end
												if (FlatIdent_1414D == 1) then
													FlatIdent_1C000 = 1;
													break;
												end
											end
											break;
										end
									end
								end
								if (FlatIdent_1C000 == 1) then
									newPosition = nil;
									while true do
										if (FlatIdent_2E48C == 1) then
											newPosition = orbitTarget.Position + offset;
											rootPart.CFrame = CFrame.lookAt(newPosition, orbitTarget.Position);
											break;
										end
										if (FlatIdent_2E48C == 0) then
											local FlatIdent_803D1 = 0;
											local FlatIdent_541BC;
											while true do
												if (FlatIdent_803D1 == 0) then
													FlatIdent_541BC = 0;
													while true do
														if (FlatIdent_541BC == 0) then
															orbitAngle = orbitAngle + math.rad(orbitSpeed * dt);
															offset = Vector3.new(math.cos(orbitAngle) * orbitDistance, orbitHeight, math.sin(orbitAngle) * orbitDistance);
															FlatIdent_541BC = 1;
														end
														if (FlatIdent_541BC == 1) then
															FlatIdent_2E48C = 1;
															break;
														end
													end
													break;
												end
											end
										end
									end
									break;
								end
							end
						end
						if (autoJump and humanoid) then
							humanoid:ChangeState(Enum.HumanoidStateType.Jumping);
						end
						if driving then
							rootPart.CFrame = rootPart.CFrame * CFrame.new(0, 0, -driveSpeed * dt);
						end
						if (banging and bangTarget) then
							local FlatIdent_4BDFE = 0;
							local FlatIdent_2CB11;
							local oscillation;
							local desiredPos;
							local humanoid;
							while true do
								if (FlatIdent_4BDFE == 0) then
									FlatIdent_2CB11 = 0;
									oscillation = nil;
									FlatIdent_4BDFE = 1;
								end
								if (FlatIdent_4BDFE == 1) then
									desiredPos = nil;
									humanoid = nil;
									FlatIdent_4BDFE = 2;
								end
								if (FlatIdent_4BDFE == 2) then
									while true do
										if (FlatIdent_2CB11 == 2) then
											humanoid = character and character:FindFirstChild("Humanoid");
											break;
										end
										if (FlatIdent_2CB11 == 1) then
											local FlatIdent_52A99 = 0;
											while true do
												if (1 == FlatIdent_52A99) then
													FlatIdent_2CB11 = 2;
													break;
												end
												if (FlatIdent_52A99 == 0) then
													desiredPos = bangTarget.Position - (bangTarget.CFrame.LookVector * (4 + oscillation));
													rootPart.CFrame = CFrame.new(desiredPos, bangTarget.Position);
													FlatIdent_52A99 = 1;
												end
											end
										end
										if (FlatIdent_2CB11 == 0) then
											local FlatIdent_827A2 = 0;
											local FlatIdent_538F0;
											while true do
												if (FlatIdent_827A2 == 0) then
													FlatIdent_538F0 = 0;
													while true do
														if (0 == FlatIdent_538F0) then
															bangTime = bangTime + dt;
															oscillation = math.sin(bangTime * bangSpeed) * 2;
															FlatIdent_538F0 = 1;
														end
														if (1 == FlatIdent_538F0) then
															FlatIdent_2CB11 = 1;
															break;
														end
													end
													break;
												end
											end
										end
									end
									break;
								end
							end
						end
						if (isBanging and targetCharacter) then
							timeElapsed = timeElapsed + dt;
							local oscillation = math.sin(timeElapsed * moveSpeed) * 2;
							local offset = Vector3.new(0, -2.5, -1 + oscillation);
							local basePos = targetCharacter.Position + targetCharacter.CFrame:VectorToWorldSpace(offset);
							local playerRootPart = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart");
							if playerRootPart then
								local FlatIdent_71F7E = 0;
								local FlatIdent_2BF13;
								local lookDirection;
								local rightVector;
								local facing;
								local humanoid;
								while true do
									if (FlatIdent_71F7E == 1) then
										local FlatIdent_866B1 = 0;
										while true do
											if (FlatIdent_866B1 == 1) then
												FlatIdent_71F7E = 2;
												break;
											end
											if (0 == FlatIdent_866B1) then
												rightVector = nil;
												facing = nil;
												FlatIdent_866B1 = 1;
											end
										end
									end
									if (FlatIdent_71F7E == 0) then
										local FlatIdent_59473 = 0;
										while true do
											if (1 == FlatIdent_59473) then
												FlatIdent_71F7E = 1;
												break;
											end
											if (FlatIdent_59473 == 0) then
												FlatIdent_2BF13 = 0;
												lookDirection = nil;
												FlatIdent_59473 = 1;
											end
										end
									end
									if (FlatIdent_71F7E == 2) then
										humanoid = nil;
										while true do
											if (FlatIdent_2BF13 == 2) then
												humanoid = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid");
												if humanoid then
													humanoid.Sit = true;
												end
												break;
											end
											if (FlatIdent_2BF13 == 1) then
												local FlatIdent_8B384 = 0;
												local FlatIdent_43143;
												while true do
													if (FlatIdent_8B384 == 0) then
