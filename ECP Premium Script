					local function disableAimbot()
						local FlatIdent_1119 = 0;
						local FlatIdent_654E4;
						local FlatIdent_5998C;
						while true do
							if (FlatIdent_1119 == 0) then
								FlatIdent_654E4 = 0;
								FlatIdent_5998C = nil;
								FlatIdent_1119 = 1;
							end
							if (1 == FlatIdent_1119) then
								while true do
									if (FlatIdent_654E4 == 0) then
										FlatIdent_5998C = 0;
										while true do
											if (1 == FlatIdent_5998C) then
												print("‚ùå Aimbot disabled!");
												break;
											end
											if (FlatIdent_5998C == 0) then
												local FlatIdent_60D2E = 0;
												while true do
													if (FlatIdent_60D2E == 1) then
														FlatIdent_5998C = 1;
														break;
													end
													if (0 == FlatIdent_60D2E) then
														aimbotEnabled = false;
														if FOVCircle then
															FOVCircle.Visible = false;
														end
														FlatIdent_60D2E = 1;
													end
												end
											end
										end
										break;
									end
								end
								break;
							end
						end
					end
					CHMSenabled = false;
					function CHMS(plr)
						task.spawn(function()
							local FlatIdent_47ABB = 0;
							while true do
								if (FlatIdent_47ABB == 1) then
									if (plr.Character and (plr.Name ~= Players.LocalPlayer.Name) and not COREGUI:FindFirstChild(plr.Name .. "_CHMS")) then
										local FlatIdent_726E7 = 0;
										local ESPholder;
										local addedFunc;
										local teamChange;
										local CHMSremoved;
										while true do
											if (2 == FlatIdent_726E7) then
												addedFunc = plr.CharacterAdded:Connect(function()
													if CHMSenabled then
														local FlatIdent_284EA = 0;
														local FlatIdent_2A75;
														while true do
															if (FlatIdent_284EA == 0) then
																FlatIdent_2A75 = 0;
																while true do
																	if (FlatIdent_2A75 == 1) then
																		local FlatIdent_627BE = 0;
																		local FlatIdent_67517;
																		while true do
																			if (FlatIdent_627BE == 0) then
																				FlatIdent_67517 = 0;
																				while true do
																					if (FlatIdent_67517 == 1) then
																						FlatIdent_2A75 = 2;
																						break;
																					end
																					if (FlatIdent_67517 == 0) then
																						repeat
																							wait(1);
																						until getRoot(plr.Character) and plr.Character:FindFirstChildOfClass("Humanoid") 
																						CHMS(plr);
																						FlatIdent_67517 = 1;
																					end
																				end
																				break;
																			end
																		end
																	end
																	if (FlatIdent_2A75 == 2) then
																		addedFunc:Disconnect();
																		break;
																	end
																	if (FlatIdent_2A75 == 0) then
																		local FlatIdent_422DB = 0;
																		local FlatIdent_2E34E;
																		while true do
																			if (FlatIdent_422DB == 0) then
																				FlatIdent_2E34E = 0;
																				while true do
																					if (FlatIdent_2E34E == 0) then
																						ESPholder:Destroy();
																						teamChange:Disconnect();
																						FlatIdent_2E34E = 1;
																					end
																					if (FlatIdent_2E34E == 1) then
																						FlatIdent_2A75 = 1;
																						break;
																					end
																				end
																				break;
																			end
																		end
																	end
																end
																break;
															end
														end
													else
														local FlatIdent_4223E = 0;
														local FlatIdent_2D5DD;
														while true do
															if (FlatIdent_4223E == 0) then
																FlatIdent_2D5DD = 0;
																while true do
																	if (FlatIdent_2D5DD == 0) then
																		teamChange:Disconnect();
																		addedFunc:Disconnect();
																		break;
																	end
																end
																break;
															end
														end
													end
												end);
												teamChange = plr:GetPropertyChangedSignal("TeamColor"):Connect(function()
													if CHMSenabled then
														local FlatIdent_2130 = 0;
														local FlatIdent_243D2;
														while true do
															if (FlatIdent_2130 == 0) then
																FlatIdent_243D2 = 0;
																while true do
																	if (FlatIdent_243D2 == 0) then
																		local FlatIdent_6679B = 0;
																		while true do
																			if (FlatIdent_6679B == 1) then
																				FlatIdent_243D2 = 1;
																				break;
																			end
																			if (FlatIdent_6679B == 0) then
																				ESPholder:Destroy();
																				addedFunc:Disconnect();
																				FlatIdent_6679B = 1;
																			end
																		end
																	end
																	if (FlatIdent_243D2 == 1) then
																		local FlatIdent_8B208 = 0;
																		local FlatIdent_2DA99;
																		while true do
																			if (FlatIdent_8B208 == 0) then
																				FlatIdent_2DA99 = 0;
																				while true do
																					if (FlatIdent_2DA99 == 1) then
																						FlatIdent_243D2 = 2;
