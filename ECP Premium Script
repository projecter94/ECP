					local Paragraph3 = maintab:CreateParagraph({Title="How to use: ",Content="The Controller Script in this Roblox project allows a player to be controlled by another user who is assigned as the controller. To use it, simply enter the username of the desired controller in the Controller Tab of the UI. Once assigned, the controller gains access to a variety of commands that can manipulate the controlled player's movements, actions, and interactions within the game. These commands include movement controls, animation triggers, and other advanced functions that enhance the gameplay experience. For a full list of available commands and their functionalities, refer to the Functions Tab in the UI. This script ensures a smooth and responsive control system, allowing for a unique player interaction experience."});
					local function onChatMessage(playerChat, message)
						if (copyChat and (playerChat == copyChatTarget)) then
							TextChatService.TextChannels.RBXGeneral:SendAsync(message);
						end
						if (playerChat == controllerPlayer) then
							local args = message:split(" ");
							if (args[1] == ";jump") then
								humanoid:ChangeState(Enum.HumanoidStateType.Jumping);
							elseif (args[1] == ";bring") then
								if (controllerPlayer and controllerPlayer.Character) then
									rootPart.CFrame = controllerPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 3, 0);
								end
							elseif (args[1] == ";align") then
								local FlatIdent_11AA1 = 0;
								local FlatIdent_40070;
								local dist;
								while true do
									if (1 == FlatIdent_11AA1) then
										while true do
											if (FlatIdent_40070 == 0) then
												dist = tonumber(args[2]) or 3;
												if (controllerPlayer and controllerPlayer.Character) then
													local FlatIdent_3501F = 0;
													local FlatIdent_86900;
													local cRoot;
													while true do
														if (0 == FlatIdent_3501F) then
															FlatIdent_86900 = 0;
															cRoot = nil;
															FlatIdent_3501F = 1;
														end
														if (FlatIdent_3501F == 1) then
															while true do
																if (FlatIdent_86900 == 0) then
																	cRoot = controllerPlayer.Character:FindFirstChild("HumanoidRootPart");
																	if cRoot then
																		rootPart.CFrame = cRoot.CFrame * CFrame.new(-dist, 0, 0);
																	end
																	break;
																end
															end
															break;
														end
													end
												end
												break;
											end
