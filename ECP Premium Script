								bg.MaxTorque = Vector3.new(8999999488, 8999999488, 8999999488);
								bg.CFrame = root.CFrame;
								local bv = Instance.new("BodyVelocity", root);
								bv.MaxForce = Vector3.new(8999999488, 8999999488, 8999999488);
								bv.Velocity = Vector3.new(0, 0.1, 0);
								local function updateFly()
									local moveDir = Vector3.zero;
									if ((ctrl.f + ctrl.b) ~= 0) then
										moveDir = moveDir + (cam.CFrame.LookVector * (ctrl.f + ctrl.b));
									end
									if ((ctrl.l + ctrl.r) ~= 0) then
										moveDir = moveDir + (cam.CFrame.RightVector * (ctrl.r + ctrl.l));
									end
									if (moveDir.Magnitude > 0) then
										bv.Velocity = moveDir.Unit * speed;
									else
										bv.Velocity = Vector3.new(0, 0.1, 0);
									end
									bg.CFrame = CFrame.new(root.Position, root.Position + cam.CFrame.LookVector);
								end
								flyState = {heartbeatConn=game:GetService("RunService").Heartbeat:Connect(updateFly),inputBeganConn=nil,inputEndedConn=nil,bg=bg,bv=bv,humanoid=humanoid};
								userInput.InputBegan:Connect(function(input)
									local FlatIdent_6D1B7 = 0;
									local FlatIdent_3F7F4;
									while true do
										if (FlatIdent_6D1B7 == 0) then
											FlatIdent_3F7F4 = 0;
											while true do
												if (0 == FlatIdent_3F7F4) then
													if userInput:GetFocusedTextBox() then
														return;
