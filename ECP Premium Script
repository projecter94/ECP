																	Fluent:Notify({Title="Evarge Hub",Content="You are now blacklisted from Evarge hub. Crashing in 3 seconds.",Duration=3});
																	FlatIdent_7705E = 1;
																end
																if (FlatIdent_7705E == 1) then
																	FlatIdent_125AB = 1;
																	break;
																end
															end
														end
													end
													break;
												end
											end
											break;
										end
										if (FlatIdent_6ACE5 == 0) then
											FlatIdent_83FD4 = 0;
											FlatIdent_125AB = nil;
											FlatIdent_6ACE5 = 1;
										end
									end
								end
								isValidKey = nil;
								FlatIdent_53341 = 1;
							end
						end
						break;
					end
				end
			end
			if (FlatIdent_95CAC == 1) then
				local FlatIdent_96AF5 = 0;
				local FlatIdent_5EEFB;
				while true do
					if (FlatIdent_96AF5 == 0) then
						FlatIdent_5EEFB = 0;
						while true do
							if (FlatIdent_5EEFB == 0) then
								local FlatIdent_7FC62 = 0;
								local FlatIdent_167F8;
								while true do
									if (0 == FlatIdent_7FC62) then
										FlatIdent_167F8 = 0;
										while true do
											if (FlatIdent_167F8 == 0) then
												blacklistFile = "blacklist.txt";
												maxAttempts = 3;
												FlatIdent_167F8 = 1;
											end
											if (FlatIdent_167F8 == 1) then
												FlatIdent_5EEFB = 1;
												break;
											end
										end
										break;
									end
								end
							end
							if (FlatIdent_5EEFB == 1) then
								canWriteFile = nil;
								FlatIdent_95CAC = 2;
								break;
							end
						end
						break;
					end
				end
			end
			if (FlatIdent_95CAC == 7) then
				handleIncorrectKey = nil;
				function handleIncorrectKey(attempts)
					attempts = attempts + 1;
					if (attempts >= maxAttempts) then
						local FlatIdent_70386 = 0;
						local FlatIdent_1E6D;
						local FlatIdent_E652;
						local success;
						local Fluent;
						while true do
							if (FlatIdent_70386 == 2) then
								while true do
									if (FlatIdent_1E6D == 1) then
										Fluent = nil;
										while true do
											if (0 == FlatIdent_E652) then
												local FlatIdent_13C9F = 0;
												local FlatIdent_2B0ED;
												while true do
													if (0 == FlatIdent_13C9F) then
														FlatIdent_2B0ED = 0;
														while true do
															if (FlatIdent_2B0ED == 1) then
																FlatIdent_E652 = 1;
																break;
															end
															if (FlatIdent_2B0ED == 0) then
																local FlatIdent_400E6 = 0;
																while true do
																	if (0 == FlatIdent_400E6) then
																		success = pcall(function()
																			writefile(blacklistFile, "BLACKLISTED");
																		end);
																		if not success then
																			print("[ERROR] Failed to write blacklist file!");
																		end
																		FlatIdent_400E6 = 1;
																	end
																	if (FlatIdent_400E6 == 1) then
																		FlatIdent_2B0ED = 1;
																		break;
																	end
																end
															end
														end
														break;
													end
												end
											end
											if (2 == FlatIdent_E652) then
												local FlatIdent_938EC = 0;
												while true do
													if (FlatIdent_938EC == 0) then
														Fluent:Notify({Title="Evarge Hub",Content="You are now blacklisted from Evarge hub. Crashing in 3 seconds.",Duration=3});
														wait(3);
														FlatIdent_938EC = 1;
													end
													if (FlatIdent_938EC == 1) then
														FlatIdent_E652 = 3;
														break;
													end
												end
											end
											if (FlatIdent_E652 == 3) then
												while true do
													print("blacklisted");
												end
												break;
											end
											if (FlatIdent_E652 == 1) then
												print("Incorrect key 3 times. You are now blacklisted.");
												Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))();
												FlatIdent_E652 = 2;
											end
										end
										break;
									end
									if (FlatIdent_1E6D == 0) then
										local FlatIdent_18379 = 0;
										while true do
											if (FlatIdent_18379 == 0) then
												FlatIdent_E652 = 0;
												success = nil;
												FlatIdent_18379 = 1;
											end
											if (FlatIdent_18379 == 1) then
												FlatIdent_1E6D = 1;
												break;
											end
										end
									end
								end
								break;
							end
							if (FlatIdent_70386 == 0) then
								FlatIdent_1E6D = 0;
								FlatIdent_E652 = nil;
								FlatIdent_70386 = 1;
							end
							if (FlatIdent_70386 == 1) then
								success = nil;
								Fluent = nil;
								FlatIdent_70386 = 2;
							end
						end
					else
						local FlatIdent_3FD4E = 0;
						local FlatIdent_4C237;
						local Fluent;
						while true do
							if (FlatIdent_3FD4E == 0) then
								FlatIdent_4C237 = 0;
								Fluent = nil;
								FlatIdent_3FD4E = 1;
							end
							if (FlatIdent_3FD4E == 1) then
								while true do
									if (2 == FlatIdent_4C237) then
										wait(math.huge);
										break;
									end
									if (FlatIdent_4C237 == 0) then
										local FlatIdent_38FB1 = 0;
										while true do
											if (FlatIdent_38FB1 == 0) then
												writeAttempts(attempts);
												Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))();
												FlatIdent_38FB1 = 1;
											end
											if (FlatIdent_38FB1 == 1) then
												FlatIdent_4C237 = 1;
												break;
											end
										end
									end
									if (FlatIdent_4C237 == 1) then
										local FlatIdent_5E074 = 0;
										local FlatIdent_833D;
										while true do
											if (FlatIdent_5E074 == 0) then
												FlatIdent_833D = 0;
												while true do
													if (FlatIdent_833D == 1) then
														FlatIdent_4C237 = 2;
														break;
													end
													if (FlatIdent_833D == 0) then
														Fluent:Notify({Title="Evarge Hub",Content=("Invalid key attempt: " .. attempts),Duration=3});
														print("Incorrect key. Try again.");
														FlatIdent_833D = 1;
													end
												end
												break;
											end
										end
									end
								end
								break;
							end
						end
					end
				end
				attempts = readAttempts();
				FlatIdent_95CAC = 8;
			end
			if (FlatIdent_95CAC == 4) then
				local FlatIdent_20D34 = 0;
				while true do
					if (FlatIdent_20D34 == 0) then
						local FlatIdent_2C966 = 0;
						while true do
							if (FlatIdent_2C966 == 1) then
								FlatIdent_20D34 = 1;
								break;
							end
							if (FlatIdent_2C966 == 0) then
								function readAttempts()
									local FlatIdent_1EAD4 = 0;
									local FlatIdent_11FEA;
									local FlatIdent_8F047;
									local success;
									local content;
									while true do
										if (2 == FlatIdent_1EAD4) then
											while true do
												if (0 == FlatIdent_11FEA) then
													local FlatIdent_547F2 = 0;
													local FlatIdent_53FC6;
													while true do
														if (0 == FlatIdent_547F2) then
															FlatIdent_53FC6 = 0;
															while true do
																if (FlatIdent_53FC6 == 1) then
																	FlatIdent_11FEA = 1;
																	break;
																end
																if (0 == FlatIdent_53FC6) then
																	FlatIdent_8F047 = 0;
																	success = nil;
																	FlatIdent_53FC6 = 1;
																end
															end
															break;
														end
													end
												end
												if (FlatIdent_11FEA == 1) then
													content = nil;
													while true do
														local FlatIdent_6E70 = 0;
														local FlatIdent_781FA;
														while true do
															if (FlatIdent_6E70 == 0) then
																FlatIdent_781FA = 0;
																while true do
																	if (FlatIdent_781FA == 0) then
																		if (FlatIdent_8F047 == 0) then
																			local FlatIdent_798CD = 0;
																			local FlatIdent_84929;
																			while true do
																				if (FlatIdent_798CD == 0) then
																					FlatIdent_84929 = 0;
																					while true do
																						if (1 == FlatIdent_84929) then
																							FlatIdent_8F047 = 1;
																							break;
																						end
																						if (FlatIdent_84929 == 0) then
																							ensureFileExists(attemptsFile, "0");
																							success, content = pcall(readfile, attemptsFile);
																							FlatIdent_84929 = 1;
																						end
																					end
																					break;
																				end
																			end
																		end
																		if (FlatIdent_8F047 == 1) then
																			return (success and tonumber(content)) or 0;
																		end
																		break;
																	end
																end
																break;
															end
														end
													end
													break;
												end
											end
											break;
										end
										if (FlatIdent_1EAD4 == 1) then
											success = nil;
											content = nil;
											FlatIdent_1EAD4 = 2;
										end
										if (FlatIdent_1EAD4 == 0) then
											FlatIdent_11FEA = 0;
											FlatIdent_8F047 = nil;
											FlatIdent_1EAD4 = 1;
										end
									end
								end
								writeAttempts = nil;
								FlatIdent_2C966 = 1;
							end
						end
					end
					if (FlatIdent_20D34 == 1) then
						function writeAttempts(attempts)
							local FlatIdent_5779E = 0;
							local FlatIdent_6FA1;
							local success;
							while true do
								if (FlatIdent_5779E == 1) then
									while true do
										if (0 == FlatIdent_6FA1) then
											success = pcall(function()
												writefile(attemptsFile, tostring(attempts));
											end);
											if not success then
												print("[ERROR] Failed to write attempts!");
											end
											break;
										end
									end
									break;
								end
								if (FlatIdent_5779E == 0) then
									FlatIdent_6FA1 = 0;
									success = nil;
									FlatIdent_5779E = 1;
								end
							end
						end
						FlatIdent_95CAC = 5;
						break;
					end
				end
			end
		end
	end
end
