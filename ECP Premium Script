					local aimbotEnabled = false;
					local FOVCircle;
					local holdingRightClick = false;
					local function createFOVCircle(fovSize)
						local FlatIdent_7F422 = 0;
						local FlatIdent_64E7F;
						while true do
							if (FlatIdent_7F422 == 0) then
								FlatIdent_64E7F = 0;
								while true do
									if (FlatIdent_64E7F == 0) then
										local FlatIdent_60A62 = 0;
										local FlatIdent_703C8;
										while true do
											if (0 == FlatIdent_60A62) then
												FlatIdent_703C8 = 0;
												while true do
													if (FlatIdent_703C8 == 0) then
														if FOVCircle then
															FOVCircle:Remove();
														end
														FOVCircle = Drawing.new("Circle");
														FlatIdent_703C8 = 1;
													end
													if (FlatIdent_703C8 == 1) then
														FlatIdent_64E7F = 1;
														break;
													end
												end
												break;
											end
										end
									end
									if (FlatIdent_64E7F == 2) then
										local FlatIdent_7C7F7 = 0;
										local FlatIdent_1B51D;
										while true do
											if (0 == FlatIdent_7C7F7) then
												FlatIdent_1B51D = 0;
												while true do
													if (FlatIdent_1B51D == 1) then
														FlatIdent_64E7F = 3;
														break;
													end
													if (FlatIdent_1B51D == 0) then
														FOVCircle.Transparency = 0.6;
														FOVCircle.Color = Color3.fromRGB(0, 255, 0);
														FlatIdent_1B51D = 1;
													end
												end
												break;
											end
										end
									end
									if (FlatIdent_64E7F == 3) then
										FOVCircle.Filled = false;
										FOVCircle.Visible = true;
										break;
									end
									if (1 == FlatIdent_64E7F) then
										local FlatIdent_50B68 = 0;
										while true do
											if (FlatIdent_50B68 == 0) then
												FOVCircle.Radius = fovSize;
												FOVCircle.Thickness = 2;
												FlatIdent_50B68 = 1;
											end
											if (FlatIdent_50B68 == 1) then
												FlatIdent_64E7F = 2;
												break;
											end
										end
									end
								end
								break;
							end
						end
					end
					local function enableAimbot(fovSize)
						local FlatIdent_8BF78 = 0;
						while true do
							if (FlatIdent_8BF78 == 2) then
